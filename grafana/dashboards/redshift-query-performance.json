        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "8.5.0",
      "targets": [
        {
          "expr": "pg_stat_activity_max_tx_duration{job=\"redshift-exporter\", datname=\"redshift_db\", state=\"active\"}",
          "interval": "",
          "legendFormat": "Max Query Duration - {{usename}}",
          "refId": "A"
